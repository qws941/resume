{
  "name": "resume",
  "version": "1.0.0",
  "description": "Personal resume management system with multi-format output",
  "main": "web/worker.js",
  "scripts": {
    "build": "cd web && node generate-worker.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "lint": "eslint .",
    "format": "prettier --write .",
    "dev": "cd web && wrangler dev",
    "deploy": "npm run build && cd web && wrangler deploy"
  },
  "keywords": [
    "resume",
    "portfolio",
    "cloudflare-workers",
    "jamstack"
  ],
  "author": "Jaecheol Lee <qws941@kakao.com>",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@types/node": "^24.7.2",
    "eslint": "^9.37.0",
    "html-minifier-terser": "^7.2.0",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "sharp": "^0.34.4",
    "wrangler": "^4.42.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
