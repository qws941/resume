{
  "service": "resume-platform",
  "strategy": "active-active-with-priority-failover",
  "regions": [
    {
      "name": "apac",
      "priority": 1,
      "healthEndpoint": "https://resume.jclee.me/health",
      "failureThreshold": 3,
      "recoveryThreshold": 2
    },
    {
      "name": "eu",
      "priority": 2,
      "healthEndpoint": "https://resume.jclee.me/health",
      "failureThreshold": 3,
      "recoveryThreshold": 2
    },
    {
      "name": "us",
      "priority": 3,
      "healthEndpoint": "https://resume.jclee.me/health",
      "failureThreshold": 3,
      "recoveryThreshold": 2
    }
  ],
  "healthCheck": {
    "intervalSeconds": 60,
    "timeoutSeconds": 10
  }
}
