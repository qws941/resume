    {
      "binding": "DB",
      "database_name": "resume-prod-db",
      "database_id": "6c723024-2527-490d-8ffd-38604b923166",
    },
  ],
  "kv_namespaces": [
    { "binding": "SESSIONS", "id": "2b81b9b02dc34f518d2ca9552804bfef" },
    { "binding": "RATE_LIMIT_KV", "id": "fe51b0f1c2c44841b4895e8747cb408a" },
